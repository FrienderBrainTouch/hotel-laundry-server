// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql" // 1. 데이터베이스 종류를 postgresql로 설정
  url      = env("DATABASE_URL") // 2. .env 파일의 DATABASE_URL 변수를 사용
}